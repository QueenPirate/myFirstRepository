<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.gridPanel{
		background: white;
		width: 50%;
		margin-left: 400px;
		
	}
	.panelPager{
			display: inline-block;
			padding-right: 20px;
	}

	</ui:style>
	<g:SimplePanel ui:field="gridPanel" addStyleNames="{style.gridPanel} center-align">
		<g:SimplePanel ui:field="pagerPanel" addStyleNames="{style.panelPager} "/>
	</g:SimplePanel>
</ui:UiBinder> 